@model List<product>

<table>
    <thead>
        <tr>
            <th>id</th>
            <th>price</th>
            <th>quantity</th>
            <th>name</th>

        </tr>
    </thead>

    @foreach (var data in Model)
    {

        {
            <tr>
                <td>@data.id</td>

                <td>@data.price</td>
                <td>@data.quantity</td>
                <td>@data.name</td>


            </tr>

        }
    }
</table>



